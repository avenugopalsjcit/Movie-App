            // dataType: "json",
            success: function (data) {

                $(".pageLoaderOverlay").hide();
                bootbox.alert("Items De-Assigned successfully");

                // alert(result);
            },
            error: function (err) {
                //  alert(err.statusText);
            }
        });
    } 
    return true;
}
