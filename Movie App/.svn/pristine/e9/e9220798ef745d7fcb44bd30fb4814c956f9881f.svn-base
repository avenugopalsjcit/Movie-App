.popoverCtnTbl > caption + thead > tr:first-child > th, .popoverCtnTbl > colgroup + thead > tr:first-child > th, .popoverCtnTbl > thead:first-child > tr:first-child > th, .popoverCtnTbl > caption + thead > tr:first-child > td, .popoverCtnTbl > colgroup + thead > tr:first-child > td, .popoverCtnTbl > thead:first-child > tr:first-child > td
