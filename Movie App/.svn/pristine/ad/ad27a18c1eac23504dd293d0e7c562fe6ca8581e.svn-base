        <div class="container">
            <h4>
                <span style="float: left; cursor: default">View Charcter Reference</span></h4>
        </div>
        <div class="container">
            <table ng-table="vm.tableParams" class="table" show-filter="true">
                <tr ng-repeat="user in $data">
                    <td filter="{ Name: 'text'}" sortable="'name'">
                        <a data-target="#updateAdminValidCharViewModal" data-toggle="modal">{{user.Name}}
                        </a>
                    </td>
                </tr>
            </table>
        </div>
        <div tabindex="-1" class="modal fade in" id="addAdminValidCharViewModal" role="dialog"
            aria-hidden="true" aria-labelledby="myModalLabel">
            <div class="vertical-alignment-helper">
                <div class="vertical-alignment-helper">
                    <div class="modal-dialog vertical-align-center" style="width: 75%;">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button class="close" type="button" data-dismiss="modal">
                                    <span aria-hidden="true">×</span><span class="sr-only">Close</span>
                                </button>
                                <h4 class="modal-title" id="H2">
                                    Add Characteristic Reference
                                </h4>
                            </div>
                            <div class="container">
                                <div class="modal-body">
                                    <div class="form-group col-md-12">
                                        <div class="col-md-4">
                                            <label class="control-label" for="inputDescription">
                                                Characteristic Type</label>
                                        </div>
                                        <div class="form-group col-md-6">
                                            <label class="control-label" for="inputDescription">
                                                7 x 24 Access Circuit Restoration</label>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <div class="col-md-4">
                                            <label class="control-label" for="Textarea1">
                                                Name</label>
                                        </div>
                                        <div class="form-group col-md-7">
                                            <textarea class="form-control" id="Textarea2" rows="4"></textarea>
                                            (Max 4000 characters are allowed in Name)
                                        </div>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <div class="col-md-4">
                                            <label class="control-label" for="comment1">
                                                Description
                                            </label>
                                        </div>
                                        <div class="form-group col-md-7">
                                            <textarea class="form-control" id="Textarea3" rows="4"></textarea>
                                            (Max 200 characters are allowed in Description)
                                        </div>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <div class="col-md-4">
                                            <label class="control-label" for="actualValueId">
                                                Value</label>
                                        </div>
                                        <div class="form-group col-md-7" id="Div1">
                                            <input class="form-control col-md-5" type="text" value="">
                                        </div>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <div class="col-md-4">
                                            <label class="control-label">
                                                Unit of Measure</label>
                                        </div>
                                        <div class="form-group col-md-7">
                                            <label>
                                                No Units of Measure on file for this Characteristic Type
                                            </label>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <div class="col-md-4">
                                            <label class="control-label" for="inputDescription">
                                                Valid</label>
                                        </div>
                                        <div class="form-group col-md-7">
                                            <input type="checkbox">
                                        </div>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <div class="col-md-4">
                                            <label class="control-label" for="inputDescription">
                                                Propagate Value to Case Details?</label>
                                        </div>
                                        <div class="form-group col-md-7">
                                            <input type="checkbox">
                                        </div>
                                    </div>
                                    <br>
                                    <br>
                                    <div class="form-horizontal" style="font-size: 13px; padding-left: 3%">
                                        <div class="form-group col-md-12">
                                            <div class="col-md-3">
                                                <u><b><span>Created by</span></b></u><br>
                                                <span>MacGamwell,John</span>
                                            </div>
                                            <div class="col-md-3">
                                                <u><b><span>Created Date</span></b></u><br>
                                                <span>21-MAY-1999 09:58:12 AM&nbsp;GMT</span>
                                            </div>
                                            <div class="col-md-3">
                                                <u><b><span>Updated by</span></b></u><br>
                                                <span>Prasad, Achutha</span>
                                            </div>
                                            <div class="col-md-3">
                                                <u><b><span>Date Last Updated</span></b></u><br>
                                                <span>18-DEC-2003 06:47:38 PM&nbsp;GMT</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
