<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <link href="../BootStrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="../BootStrap/css/styles.css" rel="stylesheet" type="text/css" />
</head>
<body ng-app="appModule">
    <div ng-controller='BTBusinessVoice'>
        <div class="panel panel-primary bg1">
            <div class="panel-body">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">
                            State/Province</label>
                        <div class="col-sm-3">
                            <select ng-change="GetCities(modproduct,modregion,modcountry,modstate)" ng-model="modstate"
                                ng-options="state.StateID as state.StateName for state in StateData" class="form-control">
                                <option value="">--- Select ---</option>
                            </select>
                            <span  style="display: none ; color:Red" class="ValidateState">Please Select State</span>
                        </div>
                        <label for="" class="col-sm-2 control-label">
                            City <span class="red"> * </span>
                        </label>
                        <div class="col-sm-3">
                            <select ng-change="GetPops(modproduct,modregion,modcountry,modstate,modCity)" ng-model="modCity"
                                ng-options="city.CityID as city.CityName for city in CityData" class="form-control">
                                <option value="">--- Select ---</option>
                            </select>
                            
                            <span  style="display: none ;color:Red" class="ValidateCity">Please Select City</span>
                        </div>
                       
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label lblPoP" ng-hide="modproduct==15">
                            PoP<span class="red"> * </span>
                        </label>
                        <div class="col-sm-3" ng-hide="modproduct==15">
                            <select ng-model="modPoP" id="ddlPoP" ng-change="GeInfoByPop()" ng-options="item.POPID as item.PopName for item in PopData"
                                class="form-control">
                                <option value="">--- Select ---</option>
                            </select>
                            <span style="display: none ;color: Red;" class="ValidatePOP">Please Select PoP</span>
                        </div>
                        <label id="" for="" class="col-sm-2 control-label lblPlatform">
                            Network Platform<span class="red"> * </span>
                        </label>
                        <div id="" class="col-sm-3 ddlPlatform">
                            <select id="ddlNetworkPlatform" ng-model="modNetworkPlatform"  ng-change="RemoveNPAlert()"
                            ng-options="item.ID as item.NetworkPlatform for item in NetworkPlatformData"
                                class="form-control">
                                <option value="">--- Select ---</option>
                            </select>
                            <span style="display: none; color: Red;" id="validateNetworkPlatform">Please Select
                                Network Platform</span>
                        </div>
                        

                        

                        <div ng-class="modproduct!=15 ? 'col-sm-2' : 'col-sm-10 text-right'">
                                <input type="button" ng-click="GetSubproductDetails()" value="GO" class="btn btn-sc" />
                         </div>


                    </div>
                    <table>
                        <tr>
                            <td>
                            </td>
                            <td>
                            </td>
                            <td>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div class="row" id="headerMsg">
            <div class="col-md-12 col-lg-12 text-center marTop10 red Arrow">
                <p>
                    {{headerMessage}}</p>
            </div>
        </div>
        <div id="documents" ng-hide="ShowSubProductProductDocLinks==0">
            <div class="panel panel-primary">
                <div class="panel-heading clickable1 upDownIconToggle collapsed" data-toggle="collapse" data-target="#PRODDOC">
                    <h3 class="panel-title">
                        Product Documents</h3>
                    <span class="pull-right"><i class="glyphicon glyphicon-chevron-down"></i>
                    </span>
                </div>
                <div id="PRODDOC" class="panel-collapse collapse">
                    <div class="panel-body">
                        <div ng-controller="DocumentLinkscntrl" id="ProductDocLinks">
                            <div class="row">
                                <div class="col-md-6 col-lg-6 text-left">
                                    <table>
                                        <tr ng-repeat="dc in DocumentslinksData">
                                            <td>
                                                <a href="{{dc.DocumentUrl}}" target="_blank">{{dc.DocumentTitle}}</a>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

         <div id="GenericVoiceChar" class="panel panel-primary" style="display: none">
            <div class="panel-heading clickable1 upDownIconToggle collapsed" data-toggle="collapse"
                data-target="#genVoiceChar">
                <h3 class="panel-title">
                    BT Generic Voice Information</h3>
                <span class="pull-right"><i class="glyphicon glyphicon-chevron-down"></i></span>
            </div>
            <div class="panel-collapse collapse" id="genVoiceChar">
                <div class="panel-body">
                    <div class="panel panel-default" ng-repeat='genVoiceChar in genericVoicechars' id="Div2">
                        <div class="panel-heading">
                            <h3 class="panel-title">
                                {{genVoiceChar.CharTypeAlias}}</h3>
                        </div>
                        <div class="panel-body">
                            <table cellpadding="0" cellspacing="0" class="table table-bordered">
                                <tr ng-repeat="subitem in genVoiceChar.chars | orderBy:'CharName'">
                                    <td width="40%" class="Arrow">
                                        {{subitem.CharName}}
                                    </td>
                                    <td width="60%" class="Arrow">
                                        {{subitem.CharValue}}
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
        <div ng-if="modproduct==15" class="text-center" style="padding-bottom:10px">
            <a ng-click="openReport()" class="clickable1">Online Reports</a>
        </div>
        <div class="panel-group" id="accordion1">
            <div class="panel panel-info" ng-repeat='item in  subProdDetails'>
                <div class="panel-heading upDownIconToggleVoice collapsed" style="cursor: pointer" ng-click="getSubProductChar(item.SubProdID,item.localCaseId,item.useCpeSubproduct)"
                    data-parent="#accordion1" data-toggle="collapse" data-target="#subProddet{{item.SubProdID}}{{$index}}">
                    <h4 class="panel-title">
                        <label>
                            {{item.SubProdName}}
                        </label>
                    </h4>
                    <span class="pull-right"><i class="glyphicon glyphicon-chevron-down"></i></span>
                </div>
                <div class="panel-collapse collapse" id="subProddet{{item.SubProdID}}{{$index}}">
                    <div class="panel-body">
                        <div class="panel panel-primary" id="Div1">
                            <div class="panel-heading clickable1 upDownIconToggle collapsed" data-toggle="collapse"
                                data-target="#collapseOne{{item.SubProdID}}">
                                <h3 class="panel-title">
                                    Product Details</h3>
                                <span class="pull-right"><i class="glyphicon glyphicon-chevron-down"></i>
                                </span>
                            </div>
                            <div id="collapseOne{{item.SubProdID}}" class="panel-collapse collapse">
                                <div class="panel-body">
                                    <table cellpadding="0" cellspacing="0" class="table table-bordered">
                                        <tr>
                                            <td width="40%" class="Arrow">
                                                Service Available
                                            </td>
                                            <td width="60%" class="Arrow">
                                                {{item.AvailabilityDesc}}
                                            </td>
                                        </tr>
                                        <tr class="showNetwork">
                                            <td width="40%" class="Arrow">
                                                Network Name
                                            </td>
                                            <td width="60%" class="Arrow">
                                                {{item.NetworkName}}
                                            </td>
                                        </tr>
                                        <tr class="resPop">
                                            <td width="40%" class="Arrow">
                                                Support Resilient Only PoP
                                            </td>
                                            <td width="60%" class="Arrow">
                                                {{item.SupprotResPOP}}
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-primary DivPopchar">
                            <div class="panel-heading clickable1 upDownIconToggle collapsed" data-toggle="collapse"
                                data-target="#collapseTwo{{item.SubProdID}}">
                                <h3 class="panel-title">
                                    PoP Characteristics</h3>
                                <span class="pull-right"><i class="glyphicon glyphicon-chevron-down"></i>
                                </span>
                            </div>
                            <div id="collapseTwo{{item.SubProdID}}" class="panel-collapse collapse">
                                <div class="panel-body">
                                    <table cellpadding="0" cellspacing="0" class="table table-bordered" id="Table1">
                                        <tr ng-repeat="pc in SubprodPopCharData">
                                            <td width="40%" class="Arrow">
                                                {{pc.Item1}}
                                            </td>
                                            <td width="60%" class="Arrow">
                                                {{pc.Item2}}
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-primary" ng-repeat='item1 in Subprodchars' id="Div3">
                            <div class="panel-heading clickable1 upDownIconToggle collapsed" data-toggle="collapse"
                                data-target="#collapseThree{{$index}}{{item.SubProdID}}">
                                <h3 class="panel-title">
                                    {{item1.CharTypeAlias}}</h3>

                                    <span class="pull-right"><i class="glyphicon glyphicon-chevron-down"></i></span>

                            </div>
                            <div id="collapseThree{{$index}}{{item.SubProdID}}" class="panel-collapse collapse">
                                <div class="panel-body">
                                    <table cellpadding="0" cellspacing="0" class="table table-bordered">
                                        <tr ng-repeat="subitem in item1.chars">
                                            <td width="40%" class="Arrow">
                                                {{subitem.CharName}}
                                            </td>
                                            <td width="60%" class="Arrow">
                                                {{subitem.CharValue}}
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div id="noDataBndle" class="alert alert-danger" role="alert" style="display: none">
                            No Data Found</div>
                        <!--SLA Info div start here-->
                        <div id="slainfoSubProd" class="row" style="display: none; padding-top:10px">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <p style="color: Red;" class="Arrow">
                                    <span style="color: Black;">SLA Information:</span> These SLAs are BT's internal
                                    SLAs with our Access Suppliers. They are <u>NOT TO BE QUOTED TO END CUSTOMERS</u>
                                    .<br />
                                    The terms of the SLA between the customer and BT is covered under the relevant BT
                                    product terms and conditions<br />
                                    for the whole service (e.g. BT MPLS).
                                </p>
                            </div>
                           
                        </div>
                        <!--<div>&nbsp;</div>-->
                        <!--SLA Info div closed here-->
                        <!--portspeed panel started-->
