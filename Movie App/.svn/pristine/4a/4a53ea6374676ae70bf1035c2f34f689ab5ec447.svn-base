                                        <span><b>{{item.SubProdName}} Port Access</b></span>
                                    </div>
                                </div>
                                <div>
                                    &nbsp;&nbsp;</div>
                                <div class="form-horizontal">
                                    <div class="form-group">
                                        <label for="" class="col-sm-2 control-label">
                                            Access Supplier(s)</label>
                                        <div class="col-sm-2">
                                            <select class="form-control" ng-model="ParentAccSupp" 
                                            ng-options="suppData.AccessSupplierID as suppData.AccessSupplierName 
                                            for suppData in subProdAccSupp" ng-change="filterSuppProd(ParentAccSupp,$event)">
                                                <option value="">All</option>
                                            </select>
                                           
                                        </div>
                                        <label for="" class="col-sm-2 control-label">
                                            Port Speed(s) <span class="red">* </span>
                                        </label>
                                        <div class="col-sm-2">
                                            <select  class="form-control" multiple size="7" ng-change="hideAlertMessage()" id="pPortSpeed"
                                             ng-options="pSpeed.PortSpeedID as pSpeed.PortSpeedName for pSpeed in subprodPortSpeedData"
                                             ng-model="subProdParentPortSpeed" style="display:none">
                                                <!--<option ng-repeat='pSpeed in subprodPortSpeedData' value="{{pSpeed.PortSpeedID}}">{{pSpeed.PortSpeedName}}</option>-->
                                            </select>
                                            <span style="display: none; color: Red;" id="spnChildPortSpeed">Please Select Port Speed</span>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="" class="col-sm-2 control-label">
                                            Access Speed(s)
                                        </label>
                                        <div class="col-sm-2">
                                            <select ng-model="ParentAccessSpeed" style="display:none" multiple size="7" id="pAccSpeed"
                                             ng-options="CAccesssupp.AccessSpeedID as CAccesssupp.AccessSpeedName for CAccesssupp in subProdAccSpeedData"
                                                class="form-control">
                                                <option ng-show="isShowAll1"  value="">All</option>
                                            </select>
                                        </div>
                                        <label for="" class="col-sm-2 control-label">
                                            Access Supplier Product(s)</label>
                                        <div class="col-sm-3">
                                            <select ng-model="ParentAccessSupplierProduct" style="display:none" multiple size="7" id="pSuppProd"
                                               ng-options="CAccesssupp.SupplierID as CAccesssupp.SupplierProductName for CAccesssupp in subProdSuppProdData"
                                                class="form-control">
                                                <option ng-show="isShowAll2" value="">All</option>
                                            </select>
                                            
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="" class="col-sm-2 control-label">
                                            Access Technology(s)
                                        </label>
                                        <div class="col-sm-2">
                                            <select ng-model="ParentAccessType" style="display:none" multiple size="7" id="pAccType"
                                            ng-options="CAccesssupp.AccessTypeID as CAccesssupp.AccessTypeName for CAccesssupp in subProdAccTypeData"
                                                class="form-control">
                                                <option ng-show="isShowAll3" value="">All</option>
                                            </select>
                                        </div>
                                        <label id="lblParentCPESuppliers" for="" class="col-sm-2 control-label" ng-hide="item.useCpeSubproduct == 0">
                                            CPE Supplier(s)</label>
                                        <div id="ParentCPESuppliers" class="col-sm-2" ng-hide="item.useCpeSubproduct == 0">
                                            <select id="ddlParentCPESupplier" ng-model="ParentCPESuppliers" ng-options="objsupp.supplierID as objsupp.SupplierName for objsupp in CpeSupplierData"
                                                class="form-control">
                                                <option value="">--- Select ---</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-2">
                                            <input class="btn btn-sc" type="button" value="GO" ng-click="GetPortSpeeds(item.localCaseId,ParentAccSupp,subProdParentPortSpeed,
                                             ParentAccessSpeed,ParentAccessSupplierProduct,ParentAccessType,ddlParentCPESuppliers,true)" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--port speed grid started here-->
                        <div>&nbsp;</div>
                        
                        <div id="Parent1Paging" class="row text-right" style="display:none">
                                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3 text-left">
                                    <label id="lblPageStatus"  ng-model="PageStatus">Page {{PageNo}} of {{PageCount}} </label>
                                </div>
                                <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9">
                                    <b><a class="Arrow" ng-click="subProdPrevParent(item.localCaseId,ParentAccSupp,subProdParentPortSpeed,
