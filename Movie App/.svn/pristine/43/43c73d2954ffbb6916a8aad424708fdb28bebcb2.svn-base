	}
@media screen and (max-width: 767px) {
	.nav-tabs > li > a {
		display:block;
	}
}


.container
{
	width:95%;	
}
.panel-primary > .panel-heading {
  color: #fff;
  background-color: RGB(6,78,116);
  border-color: RGB(6,78,116);
}
.bg-primary {
  color: #fff;
  background-color: RGB(6,78,116);
}

a {
  color:rgb(35, 82, 124);
  text-decoration: underline;
}


a:hover,
a:focus {
  color: #23527c;
  text-decoration: underline;
}
a:focus {
  outline: thin dotted;
