html, body {
    margin:0;
    padding:0;
    background-color:#FFF;
    color: #333333;
    font-family: "Arial", Helvetica, sans-serif;
    font-size:9pt;
}
/* Bootstrap */
legend {border-bottom:0 none;}
.form-control {font-size:9pt;}
.btn {font-size:9pt;}
.panel-title {font-size:9pt; font-weight:bold;}
.popoverRootTd {position:relative;overflow:visible;}
.popoverCtnTbl {position:absolute;}
.popoverCtnTbl > caption + thead > tr:first-child > th,
.popoverCtnTbl > colgroup + thead > tr:first-child > th,
.popoverCtnTbl > thead:first-child > tr:first-child > th,
.popoverCtnTbl > caption + thead > tr:first-child > td,
.popoverCtnTbl > colgroup + thead > tr:first-child > td,
.popoverCtnTbl > thead:first-child > tr:first-child > td {
  border-top:solid 1px #337ab7;
}
.popoverCtnTbl > thead > tr > th,
.popoverCtnTbl > tbody > tr > th,
.popoverCtnTbl > tfoot > tr > th,
.popoverCtnTbl > thead > tr > td,
.popoverCtnTbl > tbody > tr > td,
.popoverCtnTbl > tfoot > tr > td 
{
	padding: 0 6px;
	background-color:#E4ECF2;
	color:#000;
	border:solid 1px RGB(6,78,116);
}
.btn-link {cursor:pointer;font-size:11pt;}
.nav-tabs {
    list-style-type: none;
    margin: 0;
    padding: 0;
    border-bottom:0 none;
}
.nav-tabs > li {
    display: inline;
    margin:0;
    padding: 0;
    float:none;
}
.nav-tabs > li > a {
    background-color: RGB(6,78,116);
    border-color: RGB(6,78,116) RGB(6,78,116);
    border-radius: 3px 3px 0 0;
    border-style: solid solid none;
    border-width: 1px 1px medium;                     
    color:#FFF;
    display:inline-block;
}
.nav-tabs li a:hover
{
    border-color:RGB(6,78,116);
    background-color:#eee;
    cursor:pointer;
    color:#000;
    
}
.nav-tabs > li.active > a,
.nav-tabs > li.active > a:hover,
.nav-tabs > li.active > a:focus {
    background:#FFF;                             
    border-color: RGB(6,78,116);
    border-width: 1px 1px 0;
    color:#000;
    font-weight: normal;
    text-decoration:underline;
    cursor:pointer;
}

.oprodname
{
	padding-top: 7px;
	
}

#divUserInfo1 .table tr td, #divUserInfo .table tr td {

    width: 25%;

}

 

.tab-content {
    background-color: #FFF;
    border: 1px solid RGB(6,78,116);
    border-radius: 0 4px 4px;
    margin: -1px 0 0;
    outline: medium none;
    padding: 0;
}
.tab-pane {
	padding:10px;
}
.table th {
	background-color:RGB(6,78,116);
	color:#FFF;
}
.tblnowrap th, .tblnowrap td {white-space: nowrap;}
/* common */
.btn-sc {
	background-color:RGB(6,78,116);
	color:#FFF;
	margin: 2px 0;

}
.btn:hover,
.btn:focus,
.btn.focus {
  color: #FFF;
  text-decoration: none;
}


.clickable1 {
    cursor:pointer;
