    <div ng-show="InsertTable">
        <table class="table table-bordered" id="SLAMasterDatatable">
            <thead>
                <tr>
                    <th class="Arrow" colspan="3" style="text-align: center">
                        Primary
                    </th>
                    <th class="Arrow" colspan="3" style="text-align: center">
                        Secondary
                    </th>
                    <th class="Arrow" colspan="8">
                    </th>
                </tr>
                <tr>
                    <th class="Arrow">
                        Access Technology
                    </th>
                    <th class="Arrow">
                        Service Variant
                    </th>
                    <th class="Arrow">
                        Delivery Mode
                    </th>
                    <th class="Arrow">
                        Access Technology
                    </th>
                    <th class="Arrow">
                        Service Variant
                    </th>
                    <th class="Arrow">
                        Delivery Mode
                    </th>
                    <th class="Arrow">
                        Description
                    </th>
                    <th class="Arrow">
                        SLA Category
                    </th>
                    <th class="Arrow">
                        Site Availability Target
                    </th>
                    <th class="Arrow">
                        Severity1 Repair Target
                    </th>
                    <th class="Arrow">
