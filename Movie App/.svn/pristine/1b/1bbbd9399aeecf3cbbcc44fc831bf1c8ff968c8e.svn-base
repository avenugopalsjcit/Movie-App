<div ng-controller="EditCaseCntrl">
    <div class="form-horizontal">
        <div class="form-group">
            <label class="col-sm-2 control-label" for="">
            </label>
            <div class="col-sm-4">
                &nbsp;
            </div>
            <label class="col-sm-2 control-label" for="Details">
                <a ng-click="ShowCaseDetails()" style="font-weight: bold; text-decoration: underline;
                    color: Blue;">Details</a></label>
            <div class="col-sm-4 cus-text-left">
                <label class="col-sm-4 control-label cus-text-left" for="Case List">
                    <a ng-click="ShowCaseList()" style="font-weight: bold; text-decoration: underline;
                        color: Blue;"><< Case List</a></label>
            </div>
        </div>
        <div class="form-group" ng-show="isRegion==1">
            <label for="Region" class="col-sm-2 control-label cus-text-left">
                Region</label>
            <div class="col-sm-3 ">
                <!--<a data-target="#divShowRegions" data-toggle="modal" ng-click="GetRegions()"  style="font-weight: bold;text-decoration: underline; ">{{RegionName}}</a>
-->
                {{RegionName}}
            </div>
        </div>
        <div class="form-group" ng-show="isCountry==1">
            <label for="Country" class="col-sm-2 control-label cus-text-left">
                Country</label>
            <div class="col-sm-3 ">
                <!--<a data-target="#divShowCountries" data-toggle="modal" ng-click="GetCountries()"  style="font-weight: bold;text-decoration: underline; ">{{CountryName}}</a>
-->
                <!--<select id="Select2" ng-model="modCountry" ng-change="GetStateDetails()" ng-options="Cntry.CountryID as Cntry.CountryName for Cntry in CountryData" 
                    class="form-control">
                </select>-->
                {{CountryName}}
            </div>
        </div>
        <div class="form-group" ng-show="isState==1">
            <label for=" State/Prov" class="col-sm-2 control-label cus-text-left">
                State / Prov</label>
            <div class="col-sm-3 cus-text-left" id="divState">
                <!--<a data-target="#divState" data-toggle="modal" ng-click="GetStates()">{{StateName}}</a>-->
                <select id="ddlState" ng-model="modState" ng-options="State.StateID as State.StateName for State in StateData" style="width:90%">
                    
                </select>
            </div>
        </div>
        <div class="form-group" ng-show="isCity==1">
            <label for=" City" class="col-sm-2 control-label cus-text-left">
                City</label>
            <div class="col-sm-3 ">
                <a data-target="#divShowCities" data-toggle="modal" ng-click="GetCities()" style="font-weight: bold;
                    text-decoration: underline;">{{CityName}}</a>
            </div>
        </div>
        <div class="form-group" ng-show="isPOP==1">
            <label for=" POP" class="col-sm-2 control-label cus-text-left">
                Pop</label>
            <!-- <label for="POPName" class="col-sm-3 control-label cus-text-left">
                    {{POPName}}</label>-->
            <div class="col-sm-3 cus-text-left"  id="divPoP">
                <select id="ddlPoP" ng-model="modPoP" ng-options="pop.PoPID as pop.PoPName for pop in PoPData" style="width:90%">
                    
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for=" NetworkName" class="col-sm-2 control-label cus-text-left">
                Network Name</label>
            <label for="POPColour" class="col-sm-3 control-label cus-text-left">
                {{NetworkName}}</label>
        </div>
        <div class="form-group">
            <label for=" Availability" class="col-sm-2 control-label cus-text-left">
                Availability</label>
            <div class="col-sm-3 " id="divAvailability">
                <select id="lstBoxAvailability" ng-model="ddlAvailability" ng-options="avail.AvailID as avail.AvailDesc for avail in AvailDescData" style="width:90%">
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for=" Valid" class="col-sm-2 control-label cus-text-left">
                Valid</label>
            <div class="col-sm-3">
                <input type="checkbox" ng-model="modValid" ng-checked="CaseValid==1" />
            </div>
        </div>
        <div class="form-group">
            <label for=" Exception Required" class="col-sm-2 control-label cus-text-left">
                Exception Required</label>
            <div class="col-sm-3 cus-text-left">
                <input type="checkbox" ng-model="ExceptionRequired" ng-checked="isExceptionRequired==1" />
            </div>
        </div>
        <div class="form-group" ng-show="isCity==1">
            <label for="  Support Resilient Only POP" class="col-sm-2 control-label cus-text-left">
                Support Resilient Only POP</label>
            <div class="col-sm-3 cus-text-left">
                <input type="checkbox" ng-model="SupportResilientPOP" ng-checked="isSupportResilientPOP==1"
                    ng-model="modValid" />
            </div>
        </div>
        <div class="form-group" ng-show="isPoPColor==1">
            <label for=" PoP Colour" class="col-sm-2 control-label cus-text-left">
                PoP Colour</label>
            <label for="POPColour" class="col-sm-3 control-label cus-text-left">
                {{POPColour}}</label>
        </div>
    </div>
    <div class="form-horizontal" id="tblPopCharacteristics">
        <div class="form-group">
            <h4 class="col-sm-6 Arrow">
                PoP Characteristics
            </h4>
        </div>
        <div class="form-group col-sm-12">
            <table cellpadding="0" cellspacing="0" class="table table-bordered">
                <tr ng-repeat="popchr in POPCharacteristicsData">
                    <td width="40%" class="Arrow">
                        {{ popchr.CharName}}
                    </td>
                    <td width="60%" class="Arrow">
                        {{ popchr.CharValue}}
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <div class="form-horizontal">
        <div class="form-group">
            <label for=" Suppliers" class="col-sm-2 control-label cus-text-left">
                Suppliers</label>
            <div class="col-sm-2">
                <select id="ddlSuppliers" ng-model="Suppliers" multiple ng-multiple="true" size="5"
                    ng-options="Supp.SupplierID as Supp.SupplierName for Supp in Suppliersdata" class="form-control">
                </select>
            </div>
            <label for=" Date" class="col-sm-2 control-label cus-text-left">
                Date</label>
            <input type="text" name="inputDate" id="inputDate" ng-model="SiteAvailability" class="datepicker"
                style="width: 150px; height: 26px; margin-left: -70px" disabled="disabled" />
            <button class="btn btn-sm btn-default" id="btndate">
                <span class="glyphicon glyphicon-calendar"></span>
            </button>
        </div>
        <div class="form-group">
            <div class="col-sm-3">
            </div>
            <label for="" class="col-sm-2 control-label">
                &nbsp;</label>
            <div class="col-sm-5">
                <input type="button" ng-click="UpdateCaseInfo()" value="Update" class="btn btn-primary" />
                <button ng-click="DeleteCaseInfo()" class="btn btn-primary">
                    Delete</button>
            </div>
        </div>
    </div>
    <div>
        <div class="modal-footer">
            <table style="width: 100%;">
                <tr>
                    <th>
                        Created By
                    </th>
                    <th>
                        Created Date
                    </th>
                    <th>
                        Updated By
                    </th>
                    <th>
                        Date Last Updated
                    </th>
                </tr>
                <tr style="text-align: left;">
                    <td style="width: 25%">
                        {{CreatedBy}}
                    </td>
                    <td style="width: 25%">
                        {{CreatedDate}}
                    </td>
                    <td style="width: 25%">
                        {{UpdatedBy}}
                    </td>
                    <td style="width: 25%">
                        {{UpdatedDate}}
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <div tabindex="-1" class="modal fade in modalalign" id="divShowRegions" role="dialog"
        aria-hidden="true" aria-labelledby="myModalLabel">
        <div class="modal-dialog vertical-align-center">
            <div class="modal-content" style="width: 600px">
                <div class="modal-header" style="padding: 0px 0px 0px 0px;">
                    <button class="close" type="button" data-dismiss="modal" style="margin: 15px 15px 15px 15px;">
                        <span aria-hidden="true">×</span><span class="sr-only">Close</span>
                    </button>
                    <h3>
                        Please select Region
                    </h3>
                </div>
                <div class="container">
                    <table>
                        <tr ng-repeat="Reg in Regiondata">
                            <td>
                                <a ng-click="SelectRegion(Reg)">{{Reg.RegionName}}</a>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div tabindex="-1" class="modal fade in modalalign" id="divShowCountries" role="dialog"
        aria-hidden="true" aria-labelledby="myModalLabel">
        <div class="modal-dialog vertical-align-center">
            <div class="modal-content" style="width: 600px">
                <div class="modal-header" style="padding: 0px 0px 0px 0px;">
                    <button class="close" type="button" data-dismiss="modal" style="margin: 15px 15px 15px 15px;">
                        <span aria-hidden="true">×</span><span class="sr-only">Close</span>
                    </button>
                    <h3>
                        Please select Country
                    </h3>
                </div>
                <div class="container">
                    <table>
                        <tr ng-repeat="country in CountryData">
                            <td>
                                <a ng-click="SelectCountry(country)">{{country.CountryName}}</a>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div tabindex="-1" class="modal fade in modalalign" id="divState" role="dialog" aria-hidden="true"
        aria-labelledby="myModalLabel">
        <div class="modal-dialog vertical-align-center">
            <div class="modal-content" style="width: 600px">
                <div class="modal-header" style="padding: 0px 0px 0px 0px;">
                    <button class="close" type="button" data-dismiss="modal" style="margin: 15px 15px 15px 15px;">
                        <span aria-hidden="true">×</span><span class="sr-only">Close</span>
                    </button>
                    <h3>
                        Please select State
                    </h3>
                </div>
                <div class="container">
                    <table>
                        <tr ng-repeat="state in StateData">
                            <td>
                                <a ng-click="SelectState(state)">{{state.StateName}}</a>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div tabindex="-1" class="modal fade in modalalign" id="divShowCities" role="dialog"
        aria-hidden="true" aria-labelledby="myModalLabel">
        <div class="vertical-alignment-helper">
            <div class="modal-dialog vertical-align-center">
                <div class="modal-content">
                    <div class="modal-header">
                        <button class="close" type="button" data-dismiss="modal">
                            <span aria-hidden="true">×</span><span class="sr-only">Close</span>
                        </button>
                        <h3 class="modal-title Arrow">
                            Please select City
                        </h3>
                    </div>
                    <div class="container">
                        <div class="modal-body">
                            <div class="form-horizontal">
                                <div class="form-group col-md-12">
                                    <div class="col-sm-3">
                                        <select id="Select1" ng-model="Filter" ng-change="GetCities(true)" class="form-control">
                                            <option value="A">A</option>
                                            <option value="B">B</option>
                                            <option value="C">C</option>
                                            <option value="D">D</option>
                                            <option value="E">E</option>
                                            <option value="F">F</option>
                                            <option value="G">G</option>
                                            <option value="H">H</option>
                                            <option value="I">I</option>
                                            <option value="J">J</option>
                                            <option value="K">K</option>
                                            <option value="L">L</option>
                                            <option value="M">M</option>
                                            <option value="N">N</option>
                                            <option value="O">O</option>
                                            <option value="P">P</option>
                                            <option value="Q">Q</option>
                                            <option value="R">R</option>
                                            <option value="S">S</option>
                                            <option value="T">T</option>
                                            <option value="U">U</option>
                                            <option value="V">V</option>
                                            <option value="W">W</option>
                                            <option value="X">X</option>
                                            <option value="Y">Y</option>
                                            <option value="Z">Z</option>
                                        </select>
                                    </div>
                                    <div class="col-sm-2">
                                        <input type="text" ng-model="CitySearchText" /></div>
                                    <label class="col-sm-3">
                                        &nbsp;</label>
                                    <div class="col-sm-3">
                                        <input type="button" ng-click="SearchCity()" value="Search" /></div>
                                </div>
                                <div class="form-group col-md-12" id="City1">
                                    <label id="lblPageStatus" ng-model="PageStatus" for="pagesize" class="col-sm-3">
                                        Page {{PageNo}} of {{PageCount}}
                                    </label>
                                    <div class="col-sm-8 text-right">
                                        <a href="" ng-click="PrevParent()" style="font-weight: bold; text-decoration: underline">
                                            Prev</a>&nbsp; <a href="" ng-click="NextParent()" style="font-weight: bold; text-decoration: underline">
                                                Next</a>
                                    </div>
                                </div>
                                <div class="form-group col-md-12" ng-repeat="city in CityData">
                                    <div class="col-md-6">
                                        <a ng-click="SelectCity(city)" style="font-weight: bold; text-decoration: underline">
                                            {{city.CityName}}</a>
                                    </div>
                                </div>
                                <div class="form-group col-md-12" id="City2">
                                    <label for="" class="col-sm-3">
                                        &nbsp;</label>
                                    <div class="col-sm-8 text-right">
                                        <a href="" ng-click="PrevParent()" style="font-weight: bold; text-decoration: underline">
                                            Prev</a>&nbsp; <a href="" ng-click="NextParent()" style="font-weight: bold; text-decoration: underline">
                                                Next</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" type="button" data-dismiss="modal">
                            Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="pageLoaderOverlay">
    </div>
</div>
