<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body  ng-app="appModule">
    <div ng-controller="IPConnectCntrl">
        <div class="panel panel-primary bg1">
            <div class="panel-body">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label style="display: none" for="" class="col-sm-2 control-label">
                            BT Package/VSAT
                        </label>
                        <div style="display: none" class="col-sm-2">
                            <select id="ddlBTPackage" ng-model="BTPackage" ng-options="packageData.PackageID as packageData.PackageName for packageData in BTPackageData"
                                class="form-control">
                                <!-- <option value="">--- Select ---</option>-->
                            </select>
                            <!--<select ng-model="BTPackage" id="ddlBTPackage">
                            <option value="0">--- Select ---</option>
                            <option ng-repeat='packageData in BTPackageData' value="{{packageData.PackageID}}">{{packageData.PackageName}}</option>
                        </select>-->
                        </div>
                        <label for="" class="col-sm-2 control-label">
                            Access Supplier
                        </label>
                        <div class="col-sm-2">
                            <select id="ddlHVPNAccessSupplier" ng-model="HVPNAccessSupplier" ng-change="GetIPPortSpeed()"
                                ng-options="accSupplier.supplierID as accSupplier.SupplierName for accSupplier in HVPNAccessSuppliersData"
                                class="form-control">
                                <!--<option value="">--- Select ---</option>-->
                            </select>

                            
                                <!--<option value="">--- Select ---</option>-->
                            </select>
                            <!-- <select ng-model="HVPNAccessSupplier" ng-change="GetPortSpeedData()" id="ddlhvpnAccSupplier">
                            <option value="0">--- Select ---</option>
                            <option ng-repeat='accSupplier in HVPNAccessSuppliersData' value="{{accSupplier.supplierID}}">
                                {{accSupplier.SupplierName}}</option>
                        </select>-->
                        </div>
                        <label for="" class="col-sm-2 control-label">
                            Port Speed
                        </label>
                        <div class="col-sm-2">
                            <select id="ddlHVPNPortSpeed" ng-model="HVPNPortSpeed" ng-change="GetIPPortTypes()"
                                ng-options="ps.PortSpeedID as ps.PortSpeed for ps in HVPNPortSpeedData" class="form-control">
                                <option value="">--- Select ---</option>
                            </select>
                            <!-- <select ng-model="HVPNPortSpeed" ng-change="GetHVPNPortTypes()" id="ddlHVPNPortSpeed">
                            <option value="0">--- Select ---</option>
                            <option ng-repeat='ps in HVPNPortSpeedData' value="{{ps.PortSpeedID}}">{{ps.PortSpeed}}</option>
                        </select>-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 col-lg-6">
                <h4>
                    {{TunnelingNote}}
                </h4>
            </div>
            <div class="col-md-6 col-lg-6 text-right">
                <a class="btn-link" data-toggle="modal" ng-click="GetIPBundleData()" data-target="#viewCPEinfoModal">
                    View CPE Information</a>
                <div class="modal fade" id="viewCPEinfoModal" tabindex="-2" role="dialog">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal">
                                    &times;</button>
                                <h4 class="modal-title">
                                    CPE Bundle Details
                                </h4>
                            </div>
                            <div class="modal-body">
                                <div class="table-responsive">
                                    <table cellpadding="0" cellspacing="0" class="table table-bordered tblnowrap">
                                        <tr>
                                            <th>
                                                Router
                                            </th>
                                            <th>
                                                Access Technology
                                            </th>
                                            <th>
                                                Bundle
                                            </th>
                                            <th>
                                                Expected Date for Order
                                            </th>
                                            <th>
                                                Quotable Start Date
                                            </th>
                                            <th>
                                                EOQ
                                            </th>
                                            <th>
                                                EOL
                                            </th>
                                            <th>
                                                EOS
                                            </th>
                                            <th>
                                                SmartServiceAvailability
                                            </th>
                                        </tr>
                                        <tr ng-repeat="item1 in BundleProductsData">
                                            <td>
                                                {{ item1.Router}}
                                            </td>
                                            <td>
                                                {{item1.AccessTechnology}}
                                            </td>
                                            <td>
                                                {{item1.Bundle }} &nbsp; Parts
                                            </td>
                                            <td>
                                                {{ item1.ExpectedDate}}
                                            </td>
                                            <td>
                                                {{item1.QuotableStartDate}}
                                            </td>
                                            <td>
                                                {{item1.EOQ}}
                                            </td>
                                            <td>
                                                {{item1.EOL}}
                                            </td>
                                            <td>
                                                {{item1.EOS}}
                                            </td>
                                            <td>
                                                {{item1.SmartServiceAvailability}}
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="HvpnNoData" class="alert alert-danger" role="alert" style="display: none">
            No Data Found</div>
        <div id="HvpnTableData" class="table-responsive">
            <table id="tblHVPNData" cellpadding="0" cellspacing="0" class="table table-bordered tblnowrap">
                <tr>
                    <th>
                        BT Package
                    </th>
                    <th>
                        Access Type
                    </th>
                    <th>
                        Port Speed (Down / Up)
                    </th>
                    <th>
                        Availability
                    </th>
                    <th>
                        Access Speed (Down / Up)
                    </th>
                    <th>
                        Interface
                    </th>
                    <th>
                        Framing Structure
                    </th>
                    <th>
                        Connector
                    </th>
                    <th>
                        Access Availability Status
                    </th>
                    <th>
                        Contention Ratio
                    </th>
                    <th>
                        Supplier
                    </th>
                    <th>
                        Supplier Product/BT Internal SLA
                    </th>
                    <th>
                        Service Lead Time
                    </th>
                    <th>
                        POP 
                    </th>
                    <th>
                        CPE
                    </th>
                </tr>
                <tr ng-repeat='ps in HVPNPortTypeData'>
                    <td>
                        {{ps.PackageName}}
                    </td>
                    <td>
                        {{ps.AccessType}}
                    </td>
                    <td>
                        {{ps.PortSpeed}}
                    </td>
                    <td>
                        {{ps.Availability}}
                    </td>
                    <td>
                        {{ps.AccessSpeed}}
                    </td>
                    <td>
                        {{ps.Interface}}
                    </td>
                    <td>
                        {{ps.FramingStructure}}
                    </td>
                    <td>
                        {{ps.Connector}}
                    </td>
                    <td>
                        {{ps.AccessAvailabilityStatus}}
                    </td>
                    <td>
                        {{ps.ContentionRatio}}
                    </td>
                    <td>
                        {{ps.Supplier}}
                    </td>
                    <td>
                        {{ps.SupplierProductBTInternalSLA}}
                    </td>
                    <td>
                        {{ps.ServiceLeadTime}}
                    </td>
                    <td>
                        {{ps.POP}}
                    </td>
                    <td>
                        {{ps.CPE}}
                    </td>
                </tr>
            </table>
        </div>
    </div>
</body>
</html>
