                if ($scope.SBRName.trim() == "") {
                    $scope.SBRName = "";
                }
                else {
                    $scope.SBRName = $(element).find("#SBRId :selected").text();
                }
                $scope.MasterDetailsData.push(
                        {
                            ProductitemName: ProductName,
                            CountryName: CountryName.join(","),
