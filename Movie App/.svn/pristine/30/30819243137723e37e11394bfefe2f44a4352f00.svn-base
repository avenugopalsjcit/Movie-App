    $scope.CheckRecord = function (item, value) {
        if (value == null) {
            switch (item) {
                case "product":
                    $scope.products = true;
                    break;
                case "partner":
                    $scope.partners = true;
                    break;
                case "resilency":
                    $scope.Resilencys = true;
                    break;
                case "Countries":
                    $scope.Resilencys = true;
                    break;
            }
        }
        if (value != null) {
            switch (item) {
                case "product":
                    $scope.products = false;
                    break;
                case "partner":
                    $scope.partners = false;
                    break;
                case "resilency":
                    $scope.Resilencys = false;
                    break;
                case "Countries":
                    $scope.Resilencys = false;
                    break;
            }
        }

        if (item == 'product') {
            $scope.Countryitem = null;
            $scope.Supplieritem = null;
            $scope.Resiliencyitem = null;
            $("#PartnerId").parent().find('span').first().html('&nbsp;&nbsp--&nbsp;Select&nbsp--');
            $("#ResiliencyId").parent().find('span').first().html('&nbsp;&nbsp--&nbsp;Select&nbsp--');
            $('#SLAMasterDatatable tbody').html("<tr><td colspan='12'><label style='text-align:left'>No Records Found</label></td></tr>")
        }
    }


    var GloabalCheck = true;


    $scope.checkRec1 = function (id, item, value) {
        if (value == 'primary') {
            if (item == null) {
                $('#' + id).next().next().show();
                GloabalCheck = false;
            }
            if (item != null) {
                $('#' + id).next().next().hide();
                GloabalCheck = true;

                if ($('#' + id + ' :selected').text().trim() != "Ethernet") {
                    $('#' + id).parent().next().next().children('div').children('a').children("span:first").html("&nbsp;&nbsp;--Select--  ")
                }
            }
        }
        if (value == 'service') {
            if (item == null) {
                $('#' + id).next().next().show();
                GloabalCheck = false;
            }
            if (item != null) {
                $('#' + id).next().next().hide();
                GloabalCheck = true;
            }
        }
    }

    $scope.checkRec2 = function (id) {
