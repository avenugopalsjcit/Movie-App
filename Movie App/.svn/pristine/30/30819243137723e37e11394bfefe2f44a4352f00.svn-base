    $scope.CancelInsert = function (Id) {
        bootbox.confirm({
            "className": "my-custom-class",
            message: "Selected Record will be deleted ?",
            callback: function (result) {
                if (result) {
                    $("#cancelButton" + Id).closest('tr').remove();
                }
            }
        });
    }


