<div ng-controller="AdminDSLCaseDetailsUpdateCtrl" class="Arrow">
    <!--<div class="row text-left Arrow ">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">-->
    <!--<p class="col-sm-2 control-label" style="text-align: left">-->
    <a ng-click="GetCasesCountryAccess()" class="glyphicon glyphicon-circle-arrow-left" style="font-size: 20px;">
    </a>
    <!--           </p>-->
    <br />
    <div class="Arrow">
        <h4>
            Edit Case Details (Country Level)
        </h4>
    </div>
    <!-- </div>
    </div>-->
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="panel-body" style="padding-left: 0px;">
                <div class="form-horizontal" style="width: 100%;">
                    <div class="form-group">
                        <label class="col-sm-2 control-label cus-text-left">
                            Country &nbsp;
                        </label>
                        <p class="col-sm-2 control-label" style="text-align: left">
                            {{CountryName}}
                        </p>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label cus-text-left">
                            Availability &nbsp;
                        </label>
                        <p class="col-sm-2 control-label" style="text-align: left">
                            {{CaseAvailability}}
                        </p>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label cus-text-left">
                            Valid &nbsp;
                        </label>
                        <p class="col-sm-2 control-label" style="text-align: left">
                            [{{Validity}}]
                        </p>
                    </div>
                    <div class="form-group" id="divDLSPackages" ng-show="DLSPackages.length!=0">
                        <label for="" class="col-sm-2 control-label cus-text-left">
                            BT Package &nbsp;
                        </label>
                        <p class="col-sm-3 control-label" style="text-align: left">
                            <!--<select style="width: 120px;" id="Select2" class="form-control">
                                    <option value="Access Type">Hybrid VPN</option>
                                </select>-->
                            <select id="Select1" ng-model="DLSPackagesMod" ng-options="pkg.CharId as pkg.CharName for pkg in DLSPackages"
                                style="width: auto;" ng-change="DSLPkgChange()">
                            </select></p>
                    </div>
                    <div class="form-group" ng-hide="TunnelingNote == ''">
                        <label for="" class="col-sm-2 control-label cus-text-left">
                            Split Tunneling Availability &nbsp;
                        </label>
                        <p class="col-sm-2 control-label" style="text-align: left">
                            {{TunnelingNote}}
                        </p>
                    </div>
                    <div class="form-group" ng-show="GPOPInterconnect" id="divGPOPInterconnect">
                        <label for="" class="col-sm-2 control-label cus-text-left">
                            GPOP Interconnect&nbsp;
                        </label>
                        <p class="col-sm-4 control-label" style="text-align: left">
                            <!--<select style="width: 120px;" id="Select2" class="form-control">
                                    <option value="Access Type">Hybrid VPN</option>
                                </select>-->
                            <select id="Select2" ng-model="GPOPInterConnectMod" ng-options="gpop.HubSiteId as gpop.HubSiteName for gpop in GPOPInterconnect"
                                ng-change="GPOPChange()">                                
                            </select>
                            </p>
                    </div>
                    <div style="text-align: left; color: Blue; text-decoration: underline; font-weight: bold;">
                        <!--<a style=" color:Blue;text-decoration:underline;" ng-show="PortTypeLevelChars" ng-click="GetData()">View/Edit Port Type Level Characteristics</a>-->
                        <a data-target="#divPortTypeChars" data-toggle="modal" ng-click="GetData()" ng-show="PortTypeLevelChars">
                            View / Edit Port Type Level Characteristics</a>
                    </div>
                    <div tabindex="-1" class="modal fade in modalalign" id="divPortTypeChars" role="dialog"
                        aria-hidden="true" aria-labelledby="myModalLabel">
                        <div class="vertical-alignment-helper">
                            <div class="modal-dialog vertical-align-center">
                                <div class="modal-content" style="width: 90%;">
                                    <div class="modal-header" style="padding: 0px 0px 0px 0px;">
                                        <button class="close" type="button" data-dismiss="modal" style="margin: 15px 15px 15px 15px;">
                                            <span aria-hidden="true">×</span><span class="sr-only">Close</span>
                                        </button>
                                        <div class="row text-left Arrow container" style="padding-left: 40px">
                                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                                <h4>
                                                    Edit Case Reference (Country Level)
                                                </h4>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="container">
                                        <div id="divDSLAccessSetPortTypes">
                                            <h5 class="panel-title" ng-show="IsHybrid">
                                                Hybrid VPN Access Set - Port Types</h5>
                                            <h5 class="panel-title" ng-show="!IsHybrid">
                                                DSL Access Set - Port Types</h5>
                                            <h5 ng-show="IsHybrid" style="padding-left:15px;">
                                                Access Type - Supplier - Supplier Product - Access Speed - Interface - Framing -
                                                Connector</h5>
                                            <h5 ng-show="!IsHybrid"  style="padding-left:15px;">
                                                Access Type - Supplier - Supplier Product - GPOP Interconnect - Access Speed - Interface
                                                - Framing - Connector</h5>
                                            <select id="Select6" ng-model="AdminDetailsPortDropdownDataMod" ng-change="GetSelectedValue()"
                                                ng-options="item as item.CharName for item in AdminDetailsPortDropdownData" class="col-sm-8">
                                            </select>
                                        </div>
                                        <br />
                                        <br />
                                        <div ng-show="PortTypeCharacteristicsData != null && PortTypeCharacteristicsData.length !=0">
                                            <div class="panel-heading Arrow" style="padding-left: 0px;">
                                                <h5 class="panel-title" ng-show="IsHybrid">
                                                    Hybrid VPN Characteristics (Port Type Level)</h5>
                                                <h5 class="panel-title" ng-show="!IsHybrid">
                                                    DSL Characteristics (Port Type Level)</h5>
                                            </div>
                                            <br />
                                            <div class="form-group" ng-repeat="popchr in PortTypeCharacteristicsDataTemp">
                                                <label class="col-sm-3 control-label cus-text-left">
                                                    {{popchr.CharacteristicType}} &nbsp;
                                                </label>
                                                <p class="col-sm-2 control-label" style="text-align: left">
                                                    <select ng-model="popchr.selectedId" ng-hide="popchr.CharTypeValueCount != 3"
                                                            ng-options="case.MatrixId as case.Characteristic for case in popchr.List"
                                                            style="width: 200px;">                                                            
                                                        </select>
                                                         <select ng-model="popchr.selectedId" MULTIPLE ng-show="popchr.CharTypeValueCount != 3"
                                                            ng-options="case.MatrixId as case.Characteristic for case in popchr.List"
                                                            style="width: 200px;">                                                            
                                                        </select>
                                                </p>
                                            </div>
                                            <br />
                                            <table ng-table="PortTypeCharacteristics" class="table table-striped" id="PortTypeCharacteristicsTbl">
                                                <tr ng-repeat="popchr in $data">
                                                    <td data-title="'Characteristic Type'" filter="{ CharacteristicType: 'text'}" sortable="'CharacteristicType'"
                                                        class="Arrow">
                                                        {{ popchr.CharacteristicType}}
                                                    </td>
                                                    <td data-title="'Characteristic'" filter="{ Characteristic: 'text'}" sortable="'Characteristic'"
                                                        class="Arrow">
                                                        {{ popchr.Characteristic}}
                                                    </td>
                                                    <td data-title="'Availability'" class="Arrow">
                                                        <select id="Select7" ng-model="popchr.CharAvailCd"
                                                            ng-options="case.CaseAvailCd as case.CaseAvailDesc for case in CaseAvailabilitylst"
                                                            style="width: 200px;">
                                                            <option value="">-- Select --</option>
                                                        </select>
                                                    </td>
                                                    <td data-title="'Value'" class="Arrow">
                                                        <input ng-model="popchr.CharacteristicValue" />
                                                    </td>
                                                    <td data-title="'Valid / Invalid'" class="Arrow">
                                                        <input type="checkbox" name="chk" ng-model="popchr.IsValid" />
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                        <div ng-show="PortTypeParentCharacteristicsData != null && PortTypeParentCharacteristicsData.length !=0">
                                            <div class="panel-heading Arrow" style="padding-left: 0px;">
                                                <h5 class="panel-title" ng-show="IsHybrid">
                                                    Hybrid VPN Parent Characteristics (Port Type Level)</h5>
                                                <h5 class="panel-title" ng-show="!IsHybrid">
                                                    DSL Parent Characteristics (Port Type Level)</h5>
                                            </div>
                                            <table ng-table="PortTypeParentCharacteristics" class="table table-striped" id="PortTypeParentCharacteristicsTbl">
                                                <tr ng-repeat="popchr in $data">
                                                    <td data-title="'Characteristic Type'" filter="{ CharacteristicType: 'text'}" sortable="'CharacteristicType'"
                                                        class="Arrow">
                                                        {{ popchr.CharacteristicType}}
                                                    </td>
                                                    <td data-title="'Characteristic'" filter="{ Characteristic: 'text'}" sortable="'Characteristic'"
                                                        class="Arrow">
                                                        {{ popchr.Characteristic}}
                                                    </td>
                                                    <td data-title="'Availability'" filter="{ Availability: 'text'}" sortable="'Availability'"
                                                        class="Arrow">
                                                        {{ popchr.Availability}}
                                                    </td>
                                                    <td data-title="'Value'" filter="{ CharacteristicValue: 'text'}" sortable="'CharacteristicValue'"
                                                        class="Arrow">
                                                        {{ PortTypeParentCharacteristicsModal[$index].CharacteristicValue}}
                                                    </td>
                                                    <td data-title="'Valid / Invalid'" class="Arrow">
                                                        <input type="checkbox" name="chk" ng-model="popchr.IsValid" />
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                    <div>
                                        <div class="modal-footer">
                                            <table style="width: 100%;">
                                                <tr class="Arrow">
                                                    <th>
                                                        Created By
                                                    </th>
                                                    <th>
                                                        Created Date
                                                    </th>
                                                    <th>
                                                        Updated By
                                                    </th>
                                                    <th>
                                                        Updated Date
                                                    </th>
                                                </tr>
                                                <tr style="text-align: left;">
                                                    <td style="width: 25%">
                                                        {{CreatedBy1}}
                                                    </td>
                                                    <td style="width: 25%">
                                                        {{CreatedDate1}}
                                                    </td>
                                                    <td style="width: 25%">
                                                        {{UpdatedBy1}}
                                                    </td>
                                                    <td style="width: 25%">
                                                        {{UpdatedDate1}}
                                                    </td>
                                                </tr>
                                            </table>
                                            <input type="button" class="btn btn-primary btn-lg" ng-click="UpdatePortType()" value="Update" />
                                            <input type="button" class="btn btn-primary btn-lg" ng-click="ResetPortTypeChars()"
                                                value="Reset" />
                                            <button id="btnCaseUpdate" class="btn btn-primary btn-lg" type="button" data-dismiss="modal">
                                                Close</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="">
                <div ng-show="HVPNProductCharacteristicData != null && HVPNProductCharacteristicData.length !=0">
                    <div class="panel-heading Arrow" style="padding-left: 0px;">
                        <h5 class="panel-title" ng-show="IsHybrid">
                            HVPN Characteristics (Package Level)</h5>
                        <h5 class="panel-title" ng-show="!IsHybrid">
                            DSL Characteristics (Package Level)</h5>
                    </div>
                    
                        <table ng-table="HVPNProductCharacteristic" class="table table-striped" id="HVPNProductCharacteristicTbl">
                            <tr ng-repeat="popchr in $data">
                                <td data-title="'Characteristic Type'" filter="{ CharacteristicType: 'text'}" sortable="'CharacteristicType'"
                                    class="Arrow">
                                    {{ popchr.CharacteristicType}}
                                </td>
                                <td data-title="'Characteristic'" filter="{ Characteristic: 'text'}" sortable="'Characteristic'"
                                    class="Arrow">
                                    {{ popchr.Characteristic}}
                                </td>
                                <td data-title="'Availability'" class="Arrow">
                                    <select id="Select5" ng-model="popchr.CharAvailCd"
                                        ng-options="case.CaseAvailCd as case.CaseAvailDesc for case in CaseAvailabilitylst"
                                        style="width: 200px;">
                                        <option value="">-- Select --</option>
                                    </select>
                                </td>
                                <td data-title="'Value'" class="Arrow">
                                    <input ng-model="popchr.CharacteristicValue" />
                                </td>
                                <td data-title="'Valid / Invalid'" class="Arrow">
                                    <input type="checkbox" name="chk" ng-model="popchr.IsValid" />
                                </td>
                            </tr>
                        </table>
                    
                </div>
                <div ng-show="HVPNProductPortTypesData != null && HVPNProductPortTypesData.length !=0">
                    <div class="panel-heading Arrow" style="padding-left: 0px;">
                        <h5 class="panel-title" ng-show="IsHybrid">
                            Hybrid VPN Port Types</h5>
                        <h5 class="panel-title" ng-show="!IsHybrid">
                            DSL Port Types</h5>
                    </div>
                    <table ng-table="HVPNProductPortTypes" class="table table-striped" id="HVPNProductPortTypesTbl">
                        <tr ng-repeat="popchr in $data" id="HVPNProductPortTypes{{$index}}">
                            <td data-title="'Port Speed (Down/Up)'" filter="{ PortSpeed: 'text'}" sortable="'PortSpeed'"
                                class="Arrow" ng-show="IsHybrid">
                                {{ popchr.PortSpeed}}
                            </td>
                            <td data-title="'Port Speed Availability'" class="Arrow" ng-show="IsHybrid">
                                <select id="ddlCaseAvailability[$Index]" ng-model="popchr.PortSpeedAvailabilityId"
                                    ng-options="case.CaseAvailCd as case.CaseAvailDesc for case in CaseAvailabilitylst"
                                    style="width: 200px;">
                                    <option value="">-- Select --</option>
                                </select>
                            </td>
                            <td data-title="'Characteristic Type'" filter="{ CharacteristicType: 'text'}" sortable="'CharacteristicType'"
                                class="Arrow" ng-show="!IsHybrid">
                                {{ popchr.CharacteristicType}}
                            </td>
                            <td data-title="'Access Availability'" class="Arrow" ng-show="!IsHybrid">
                                <select id="Select3" ng-model="popchr.AccessAvailabilityId"
                                    id="HVPNProductPortTypesAvai[$Index]" ng-options="case.CaseAvailCd as case.CaseAvailDesc for case in CaseAvailabilitylst"
                                    style="width: 200px;">
                                    <option value="">-- Select --</option>
                                </select>
                            </td>
                            <td data-title="'Access Type'" filter="{ AccessType: 'text'}" sortable="'AccessType'"
                                class="Arrow">
                                {{ popchr.AccessType}}
                            </td>
                            <td data-title="'DSL Technology Type'" class="Arrow" ng-show="!IsHybrid">
                                <select id="Select4" ng-model="popchr.DSLTechnologyTypeId"
                                    ng-options="case.CaseAvailCd as case.CaseAvailDesc for case in CaseAvailabilitylst"
                                    style="width: 200px;">
                                    <option value="">-- Select --</option>
                                </select>
                            </td>
                            <td data-title="'Supplier'" filter="{ Supplier: 'text'}" sortable="'Supplier'" class="Arrow">
                                {{ popchr.Supplier}}
                            </td>
                            <td data-title="'Supplier Product'" filter="{ SupplierProduct: 'text'}" sortable="'SupplierProduct'"
                                class="Arrow">
                                {{ popchr.SupplierProduct}}
                            </td>
                            <td data-title="'GPOP Interconnect'" filter="{ GPOPInterconnect: 'text'}" sortable="'GPOPInterconnect'"
                                class="Arrow" ng-show="IsHybrid">
                                {{ popchr.GPOPInterconnect}}
                            </td>
                            <td data-title="'Interconnect Design'" filter="{ InterconnectDesign: 'text'}" sortable="'InterconnectDesign'"
                                class="Arrow" ng-show="!IsHybrid">
                                {{ popchr.InterconnectDesign}}
                            </td>
                            <td data-title="'Access Availability'" class="Arrow" ng-show="IsHybrid">
                                <select id="ddlCaseAvailability1" ng-model="popchr.AccessAvailabilityId"
                                    ng-options="case.CaseAvailCd as case.CaseAvailDesc for case in CaseAvailabilitylst"
                                    style="width: 200px;">
                                    <option value="">-- Select --</option>
                                </select>
                            </td>
                            <td data-title="'Access Speed(Down / Up)'" filter="{ AccessSpeed: 'text'}" sortable="'AccessSpeed'"
                                class="Arrow">
                                {{ popchr.AccessSpeed}}
                            </td>
                            <td data-title="'Interface'" filter="{ Interface: 'text'}" sortable="'Interface'"
                                class="Arrow">
                                {{ popchr.Interface}}
                            </td>
                            <td data-title="'Framing'" filter="{ Framing: 'text'}" sortable="'Framing'" class="Arrow">
                                {{popchr.Framing}}
                            </td>
                            <td data-title="'Connector'" filter="{ Connector: 'text'}" sortable="'Connector'"
                                class="Arrow">
                                {{popchr.Connector}}
                            </td>
                            <td data-title="'Valid / Invalid'" class="Arrow">
                                <input type="checkbox" name="chk" ng-model="popchr.IsValid" />
                            </td>
                           <!--  <td>          
                              <span>
                                <button class="btn btn-default btn-sm" ng-click="EditRecord('HVPNProductPortTypes'+$index)"><span class="glyphicon glyphicon-pencil"></span></button>                                </span>
                                <span style="display:none;"><button class="btn btn-primary btn-sm" ng-click="UpdateRecord(popchr)"><span class="glyphicon glyphicon-ok"></span></button>
                               <button class="btn btn-default btn-sm" ng-click="CancelUpdate('HVPNProductPortTypes'+$index,x.UserCategoryDesc)"><span class="glyphicon glyphicon-remove"></span></button></span>
                              </td>   -->               
                        </tr>
                    </table>
                </div>
                <div ng-show="HVPNProductParentCharacteristicData != null && HVPNProductParentCharacteristicData.length !=0">
                    <div class="panel-heading Arrow" style="padding-left: 0px;">
                        <h5 class="panel-title" ng-show="IsHybrid">
                            Hybrid VPN Parent Characteristics (Package Level)</h5>
                        <h5 class="panel-title" ng-show="!IsHybrid">
                            DSL Parent Characteristics (Package Level)</h5>
                    </div>
                    
                        <table ng-table="HVPNProductParentCharacteristic" class="table table-striped" id="HVPNProductParentCharacteristicTbl">
                            <tr ng-repeat="popchr in $data">
                                <td data-title="'Characteristic Type'" filter="{ CharacteristicType: 'text'}" sortable="'CharacteristicType'"
                                    class="Arrow">
                                    {{ popchr.CharacteristicType}}
                                </td>
                                <td data-title="'Characteristic'" filter="{ Characteristic: 'text'}" sortable="'Characteristic'"
                                    class="Arrow">
                                    {{ popchr.Characteristic}}
                                </td>
                                <td data-title="'Availability'" filter="{ Availability: 'text'}" sortable="'Availability'"
                                    class="Arrow">
                                    {{ popchr.Availability}}
                                </td>
                                <td data-title="'Value'" filter="{ CharacteristicValue: 'text'}" sortable="'CharacteristicValue'"
                                    class="Arrow">
                                    {{ popchr.CharacteristicValue}}
                                </td>
                                <td data-title="'Valid / Invalid'" class="Arrow">
                                    <input type="checkbox" name="chk" ng-model="popchr.IsValid" />
                                </td>
                            </tr>
                        </table>
                        
                </div>
                <div ng-show="HVPNProductParentPortTypesData != null && HVPNProductParentPortTypesData.length !=0">
                    <div class="panel-heading Arrow" style="padding-left: 0px;">
                        <h5 class="panel-title" ng-show="IsHybrid">
                            Hybrid VPN Parent Port Types</h5>
                        <h5 class="panel-title" ng-show="!IsHybrid">
                            DSL Parent Port Types</h5>
                    </div>
                    <div class="panel-body">
                        <table ng-table="HVPNProductParentPortTypes" class="table table-striped" id="HVPNProductParentPortTypesTbl">
                            <tr ng-repeat="popchr in $data">
                                <td data-title="'Port Speed (Down/Up)'" filter="{ PortSpeed: 'text'}" sortable="'PortSpeed'"
                                    class="Arrow" ng-show="IsHybrid">
                                    {{ popchr.PortSpeed}}
                                </td>
                                <td data-title="'Port Speed Availability'" filter="{ PortSpeedAvailability: 'text'}"
                                    sortable="'PortSpeedAvailability'" class="Arrow" ng-show="IsHybrid">
                                    {{ popchr.PortSpeedAvailability}}
                                </td>
                                <td data-title="'Characteristic Type'" filter="{ CharacteristicType: 'text'}" sortable="'CharacteristicType'"
                                    class="Arrow" ng-show="!IsHybrid">
                                    {{ popchr.CharacteristicType}}
                                </td>
                                <td data-title="'Access Availability'" filter="{ AccessAvailability: 'text'}" sortable="'AccessAvailability'"
                                    class="Arrow" ng-show="!IsHybrid">
                                    {{ popchr.AccessAvailability}}
                                </td>
                                <td data-title="'Access Type'" filter="{ AccessType: 'text'}" sortable="'AccessType'"
                                    class="Arrow">
                                    {{ popchr.AccessType}}
                                </td>
                                <td data-title="'DSL Technology Type'" filter="{ DSLTechnologyType: 'text'}" sortable="'DSLTechnologyType'"
                                    class="Arrow" ng-show="!IsHybrid">
                                    {{ popchr.DSLTechnologyType}}
                                </td>
                                <td data-title="'Supplier'" filter="{ Supplier: 'text'}" sortable="'Supplier'" class="Arrow">
                                    {{ popchr.Supplier}}
                                </td>
                                <td data-title="'Supplier Product'" filter="{ SupplierProduct: 'text'}" sortable="'SupplierProduct'"
                                    class="Arrow">
                                    {{ popchr.SupplierProduct}}
                                </td>
                                <td data-title="'GPOP Interconnect'" filter="{ GPOPInterconnect: 'text'}" sortable="'GPOPInterconnect'"
                                    class="Arrow" ng-show="IsHybrid">
                                    {{ popchr.GPOPInterconnect}}
                                </td>
                                <td data-title="'Interconnect Design'" filter="{ InterconnectDesign: 'text'}" sortable="'InterconnectDesign'"
                                    class="Arrow" ng-show="!IsHybrid">
                                    {{ popchr.InterconnectDesign}}
                                </td>
                                <td data-title="'Access Availability'" filter="{ AccessAvailability: 'text'}" sortable="'AccessAvailability'"
                                    class="Arrow" ng-show="IsHybrid">
                                    {{ popchr.AccessAvailability}}
                                </td>
                                <td data-title="'Access Speed(Down / Up)'" filter="{ AccessSpeed: 'text'}" sortable="'AccessSpeed'"
                                    class="Arrow">
                                    {{ popchr.AccessSpeed}}
                                </td>
                                <td data-title="'Interface'" filter="{ Interface: 'text'}" sortable="'Interface'"
                                    class="Arrow">
                                    {{ popchr.Interface}}
                                </td>
                                <td data-title="'Framing'" filter="{ Framing: 'text'}" sortable="'Framing'" class="Arrow">
                                    {{popchr.Framing}}
                                </td>
                                <td data-title="'Connector'" filter="{ Connector: 'text'}" sortable="'Connector'"
                                    class="Arrow">
                                    {{popchr.Connector}}
                                </td>
                                <td data-title="'Valid / Invalid'" class="Arrow">
                                    <input type="checkbox" name="chk" ng-model="popchr.IsValid"
                                        id="Checkbox1" />
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" ng-show="DLSPackages.length!=0">
            <div class="panel-body">
                <div class="modal-footer">
                    <div>
                        <table style="width: 100%;">
                            <tr>
                                <th>
                                    Created By
                                </th>
                                <th>
                                    Created Date
                                </th>
                                <th>
                                    Updated By
                                </th>
                                <th>
                                    Updated Date
                                </th>
                            </tr>
                            <tr style="text-align: left;">
                                <td style="width: 25%">
                                    {{CreatedBy}}
                                </td>
                                <td style="width: 25%">
                                    {{CreatedDate}}
                                </td>
                                <td style="width: 25%">
                                    {{UpdatedBy}}
                                </td>
                                <td style="width: 25%">
                                    {{UpdatedDate}}
                                </td>
                            </tr>
                        </table>
                    </div>
                    <input type="button" class="btn btn-primary btn-lg" value="Update" ng-click="Update()" />
                    <input type="button" class="btn btn-primary btn-lg" ng-click="ResetCaseDetails()"
                        value="Reset" />
                </div>
            </div>
        </div>
    </div>
</div>
<style type="text/css">

    
    input 
{
    font-weight:normal !important;
    }
</style>