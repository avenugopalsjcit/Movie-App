    <br />
    <table ng-table="vm.tableParams" class="table" show-filter="true">
        <tr ng-repeat="user in $data">
            <td title="'Product'" filter="{ Country: 'text'}" sortable="'Country'" groupable="'Country'">
                <a data-toggle="modal" data-target="#ProductMappingModal">{{user.Country}} </a>
            </td>
            <td title="'Distributor'" filter="{ ManufacturerName: 'text'}" sortable="'ManufacturerName'">
