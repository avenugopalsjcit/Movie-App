function getStringArray(obj, field) {
    return obj.map(function (item) {
        return item[field];
    });
}

function checkVPoP(charList) {
    charList.forEach(function (item) {
        if (item.popType == "VPOP") {
            return 1;
        }
    });
    return 0;
}

function toggleIcon(icon) {
    var iconSymbol = (icon == "+")
            ? "-"
            : "+";
    return iconSymbol;
}

function checkCaseSelection() {
    if ($('.wTreeStyle input:checked').length == 0) {
        bootbox.alert("Please make a selection in the Case Selection box");
        return;
    }
}

function GetUniqueArray(items, propertyName) {
    var result = [];
    var prevVal = '';

    items.forEach(function (item) {
        if (item[propertyName] != prevVal) {
            result.push(item);
            prevVal = item[propertyName];
        }
    });
    return result;
}

function GetSingleUniqueArray(items, propertyName) {
    var result = [];
    $.each(items, function (index, item) {
        if ($.inArray(item[propertyName], getStringArray(result, propertyName)) == -1) {
            var obj = {};
            obj[propertyName] = item[propertyName];
            result.push(obj);
        }
    });
    return result;
}

function toggleFolderImage(imgID) {
    var src = ($("#" + imgID).attr('src') == "image/closefolder.gif")
            ? "image/openfolder.gif"
            : "image/closefolder.gif";
    $("#" + imgID).attr('src', src);
    return src;
}

var checkParents = function (c) {
    var parentLi = c.parents('ul:eq(0)').parents('li:eq(0)');

    if (parentLi.length) {
        var siblingsChecked = parseInt($('input[type="checkbox"]:checked', c.parents('ul:eq(0)')).length),
                        rootCheckbox = parentLi.find('input[type="checkbox"]:eq(0)')
                    ;


        var id = c.attr('id');
        var divid = $("#" + id).closest("div")[0].id;
        if (divid != '') {
            var all_checkboxes = $('#' + divid + ' input[type="checkbox"]');
        }

        if (c.is(':checked') && (all_checkboxes.length === all_checkboxes.filter(":checked").length))
            rootCheckbox.prop('checked', true)
        else
            rootCheckbox.prop('checked', false);

        checkParents(rootCheckbox);
    }
}