                                    <td width="60%" class="Arrow">
                                        {{PopCode}}
                                    </td>
                                </tr>
                               

                                <tr ng-show="TargetAvailabilityDate!=''">
                                    <td class="Arrow">
                                        Target Availability Date 
                                    </td>
                                    <td class="Arrow">
                                        {{TargetAvailabilityDate}}
                                    </td>
                                </tr>

                                <tr>
                                    <td width="40%" class="Arrow">
                                        Network Name 
                                    </td>
                                    <td width="60%" class="Arrow">
                                        {{NetworkName}}
                                    </td>
                                </tr>
